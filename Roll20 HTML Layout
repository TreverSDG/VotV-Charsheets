                                on('sheet:opened change:fuelcur1 change:fuelmax1', function() {
                                    getAttrs(['fuelcur1', 'fuelmax1'], function(values) {
                                        // make sure the maximum and current values are numbers; using +value || 0 is one way to do this.
                                        const maximum = +values.fuelmax1 || 0;
                                        const current = +values.fuelcur1 || 0;
                                        // get the percentage value
                                        const percent = (current / maximum) || 0;
                                        // it looks like you want to round up to increments of 10%, so 33% would be reported as 4.
                                        // the following does that, and if over 100% reports 999.
                                        const tvalue = percent > 1 ? 999 : Math.ceil(percent * 10);
                                        console.log('fuelic value is ' + tvalue);
                                
                                        setAttrs({
                                            'fuelicv1':tvalue
                                        });
                                    });
                                });
